<h1> Edit Studio</h1>



<form method ="post" action="/Studio/update?_method=PUT">

    <label>Name</label>
    <input type="text" name="name" value = <%= Studio.name %> class="" >

    <label>Package Type</label>
    <input type="number" name="packageType" value = <%= Studio.packageType %> class="" >

    <label>description</label>
    <input type="number" name="price" value = <%= Studio.description %> class="" >
    
    <label>Price</label>
    <input type="number" name="price" value = <%= Studio.price %> class="" >

    <label>Image</label>
    <input type="file" name="image"  value = <%= Studio.image %> class="" >


    <input type="hidden" name="id" value="<%= Studio._id %>"  class="form-control">
    <br>
    <button type="submit" class="">Save</button>
</form>